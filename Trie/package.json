{
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "@types/jest": "^23.3.13"
  },
  "devDependencies": {
    "jest": "^23.0.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.4"
  }
}
